    "import math\n",
    "import random\n",
    "\n",
    "n = 10\n",
    "lmbd = 2\n",
    "\n",
    "\n",
    "y = []\n",
    "\n",
    "def get_y (x):\n",
    "    return (-1)*(math.log(1-x)/lmbd)\n",
    "\n",
    "\n",
    "for i in range(0,n):\n",
    "    y.append( get_y(random.randrange(0, 100, 1)/100) )\n",
    "\n"
